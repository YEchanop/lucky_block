  "authors": [
    "creoii"
  ],
  "contact": {
    "homepage": "https://creoii.dev/projects/modern-lucky-block",
    "sources": "https://github.com/jack-zisa/lucky_block"
  },
  "license": "CC0-1.0",
  "icon": "assets/lucky/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "dev.creoii.luckyblock.fabric.LuckyBlockFabric"
    ],
    "client": [
      "dev.creoii.luckyblock.fabric.client.LuckyBlockFabricClient"
    ]
  },
  "accessWidener": "lucky.accesswidener",
  "mixins": [
    "lucky.mixins.json"
